{
    "name": "indieopen-frontend",
    "description": "INDIeOpen repository for creating learning units within the INDIe Project",
    "version": "1.6.8",
    "repository": {
        "type": "GIT",
        "url": "https://MY_REPOSITORY"
    },
    "scripts": {
        "update-dependencies": "ncu -u -x rxjs,bootstrap,typescript",
        "build": "npm run build:pug && npm run ng -- build --configuration production && npm run build:version",
        "build:staging": "npm run build:pug && npm run ng -- build --configuration staging && npm run build:version",
        "build:debug": "npm run build:pug && npm run ng -- build --configuration production --source-map && npm run build:version",
        "build:pug": "node scripts/build-pug.js",
        "build:version": "node scripts/version.js",
        "bundle-report": "webpack-bundle-analyzer dist/indieopen-frontend/stats-es2015.json",
        "e2e": "npm run ng -- e2e",
        "lint:fix": "npm run ng -- lint --fix",
        "lint": "npm run ng -- lint",
        "ng": "cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node_modules/.bin/ng",
        "serve": "./node_modules/.bin/static-server dist/indieopen-frontend",
        "start": "npm run ng -- serve --open",
        "start:dev": "node scripts/start.js",
        "test": "npm run ng -- test"
    },
    "private": true,
    "dependencies": {
        "@angular-slider/ngx-slider": "^2.0.3",
        "@angular/animations": "^12.1.3",
        "@angular/cdk": "^12.1.3",
        "@angular/common": "12.1.3",
        "@angular/compiler": "12.1.3",
        "@angular/core": "12.1.3",
        "@angular/forms": "12.1.3",
        "@angular/localize": "^12.1.3",
        "@angular/material": "^12.1.3",
        "@angular/platform-browser": "12.1.3",
        "@angular/platform-browser-dynamic": "12.1.3",
        "@angular/router": "12.1.3",
        "@fortawesome/angular-fontawesome": "0.9.0",
        "@fortawesome/fontawesome-pro": "^6.1.1",
        "@fortawesome/fontawesome-svg-core": "1.2.35",
        "@fortawesome/free-brands-svg-icons": "5.15.3",
        "@fortawesome/free-regular-svg-icons": "5.15.3",
        "@fortawesome/free-solid-svg-icons": "5.15.3",
        "@ng-bootstrap/ng-bootstrap": "10.0.0",
        "@ng-select/ng-select": "^7.2.0",
        "@ngx-translate/core": "13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "cookieconsent": "^3.1.1",
        "date-fns": "^2.22.1",
        "flag-icon-css": "^3.5.0",
        "html-webpack-plugin": "^5.3.2",
        "messageformat": "^2.3.0",
        "moment": "^2.29.1",
        "ng-packagr": "^12.1.2",
        "ngx-chips": "^2.2.2",
        "ngx-clipboard": "^14.0.1",
        "ngx-cookie-service": "^12.0.0",
        "ngx-material-rating": "^2.0.0",
        "ngx-range": "^1.1.0",
        "ngx-toastr": "^14.0.0",
        "node-forge": "^0.10.0",
        "object-hash": "2.2.0",
        "rxjs": "^6.6.7",
        "tslib": "2.3.0",
        "uuid": "8.3.2",
        "webpack-bundle-analyzer": "4.4.2",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "12.1.3",
        "@angular/cli": "12.1.3",
        "@angular/compiler-cli": "12.1.3",
        "@angular/language-service": "12.1.3",
        "@inip/static-server": "1.0.1",
        "@prettier/plugin-pug": "1.16.1",
        "@types/moment": "^2.13.0",
        "@types/node": "^16.4.0",
        "@types/node-forge": "^0.10.2",
        "@types/object-hash": "2.1.1",
        "@types/uuid": "8.3.1",
        "bootstrap": "4.5.0",
        "chokidar": "3.5.2",
        "codelyzer": "6.0.2",
        "concurrently": "6.2.0",
        "cross-env": "7.0.3",
        "prettier": "2.3.2",
        "protractor": "7.0.0",
        "pug": "3.0.2",
        "pug-lint": "2.6.0",
        "shelljs": "0.8.4",
        "ts-node": "10.1.0",
        "tslint": "6.1.3",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "4.2.4"
    }
}
